{
    "manifest_version": 2,
    "name": "Security+ Password Manager",
    "version": "1.0",
    "permissions": ["activeTab", "storage", "<all_urls>", "identity"],
    "background": {
      "scripts": ["js/background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["js/content.js"]
      }
    ],
    "browser_action": {
      "default_popup": "pages/popup.html"
    }
  }
  